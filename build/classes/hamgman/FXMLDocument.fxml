<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="414.0" prefWidth="509.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="hamgman.FXMLDocumentController">
   <children>
      <ImageView fitHeight="414.0" fitWidth="509.0" pickOnBounds="true">
         <image>
            <Image url="@../b.gif" />
         </image>
      </ImageView>
      <Button fx:id="playButton" layoutX="118.0" layoutY="314.0" mnemonicParsing="false" onAction="#handlePlayAction" prefHeight="51.0" prefWidth="91.0" style="-fx-background-color: white;" text="PLAY" textFill="#06a852">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="322.0" layoutY="314.0" mnemonicParsing="false" onAction="#handleExitAction" prefHeight="51.0" prefWidth="91.0" style="-fx-background-color: white;" text="EXIT" textAlignment="CENTER" textFill="#06a852">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <ImageView fitHeight="94.0" fitWidth="200.0" layoutX="155.0" layoutY="21.0" pickOnBounds="true">
         <image>
            <Image url="@../chalkboard.gif" />
         </image>
      </ImageView>
      <Label layoutX="114.0" layoutY="7.0" prefHeight="70.0" prefWidth="287.0" text="LET's   PLAY " textFill="WHITE">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
      <Label layoutX="118.0" layoutY="58.0" text="HANGMAN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
